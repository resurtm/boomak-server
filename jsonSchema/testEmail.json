{
  "$schema": "http://json-schema.org/schema#",
  "description": "Validates test email job data",
  "type": "object",
  "properties": {
    "recipientEmail": {
      "type": "string",
      "minLength": 5,
      "maxLength": 50,
      "format": "email"
    },
    "testString": {
      "type": "string",
      "minLength": 6,
      "maxLength": 255
    }
  },
  "minProperties": 2,
  "maxProperties": 2,
  "required": [
    "recipientEmail",
    "testString"
  ]
}
